<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<style>
    h1 {
text-align: center;
margin-top:10px;
margin-bottom:50px;
}
</style>
<body style="background-color:lavenderblush">
    <div>
        <h1 style="font-family:Trebuchet MS; font-size:50px;">User Records</h1>
    </div>
    <div class="container" style="background-color:white">
        <table class="table table-bordered table-border text-center shadow">
            <tr>
                <th> user id</th>
                <th> user name</th>
                <th> user password</th>
                <th> user email</th>
                <th> user type</th>
            </tr>
            <% add_students.forEach(function(userr){ %>
            <tr>
                <td><%= userr.id %></td>
                <td><%= userr.name %></td>
                <td><%= userr.password %></td>
                <td><%= userr.email %></td>
                <td><%= userr.type %></td>
                <td><a href ="/Delete/<%= userr.id %>" class="btn btn-danger btn-sm">delete</a></td>
                <td><a href ="/edit/<%= userr.id %>" class="btn btn-success btn-sm">edit</a></td>           
            </tr>
            <% }) %>
        </table>
    </div>
</body>
</html>